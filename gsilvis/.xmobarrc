Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = TopW L 95
       , lowerOnStart = True
       , commands = [ Run Weather "KBOS" ["-t","<tempF>F, <rh>%", "-L", "32", "-H", "78", "--low", "blue", "--high", "red"] 600
                    , Run Wireless "wlan1" ["-L", "40", "-H", "70", "--low", "red", "--normal", "yellow", "--high", "green", "-w", "2"] 10
                    , Run Battery ["-L", "15", "-H", "70", "--low", "red", "--high", "green"] 600
                    , Run Cpu ["-t", "<total>%", "-L","3","-H","50","--normal","green","--high","red", "-w", "2"] 10
                    , Run Date "%a %D, %R" "date" 10
                    , Run StdinReader
                    , Run CommandReader "/home/george/.ledmon/ledmon" "LED"
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ <fc=#ff0000>%LED%</fc>| %KBOS% | %cpu% | <fc=#ee9a00>%date%</fc>"
       }

Config { font = "-*-Fixed-Normal-R-Normal-*-12-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = TopW L 100
       , lowerOnStart = True
       , commands = [ Run MultiCpu ["-t", "(<total0>, <total1>)", "-L", "25", "-H", "75", "--normal", "white", "--high", "orange", "-m", "3", "-w", "3"] 10
                    , Run Memory ["-t", "<used>MB", "-L", "1000", "-H", "2500", "--normal", "white", "--high", "orange", "-c", "0", "-m", "4", "-w", "4"] 10
                    , Run Date "%a %Y %b %d %R" "date" 10
                    , Run CommandReader "acpi -b | tr -d ',' | sed -e 's/100/FF/' | cut -b 12-29 | cut --complement -b 14" "battery"
                    , Run StdinReader
                    , Run CommandReader "/home/frozencemetery/.mpdmonitor.sh" "MPD"
                    , Run CommandReader "/home/frozencemetery/.alsamonitor.sh" "ALSA"
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%date% | %memory% | %battery% | %multicpu% | %StdinReader% | (%ALSA%) %MPD%}{"
       }

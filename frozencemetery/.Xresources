URxvt*saveLines: 12000
URxvt*scrollBar: false
URxvt*scrollstyle: rxvt

URxvt*urlLauncher: iceweasel
URxvt*matcher.button: 1

URxvt*secondaryWheel: true

URxvt*font: xft:Terminus:size=8
URxvt*boldFont: xft:Terminus:size=8
URxvt*italicFont: xft:Terminus:size=8
URxvt*letterSpace: 0;

! that this is required drives me crazy
URxvt*modifier: mod1

! zanbarn
URxvt*termName: xterm-color
URxvt*foreground: #FFFFFF
URxvt*cursorColor: magenta
URxvt*colorUL: #c5f779
URxvt*underlineColor: #c5f779
URxvt*color0: #000000
URxvt*color1: #9e1828
URxvt*color2: #aece92
URxvt*color3: #968a38
URxvt*color4: #414171
URxvt*color5: #963c59
URxvt*color6: #418179
URxvt*color7: #bebebe
URxvt*color8: #666666
URxvt*color9: #cf6171
URxvt*color10: #c5f779
URxvt*color11: #fff796
URxvt*color12: #4186be
URxvt*color13: #cf9ebe
URxvt*color14: #71bebe
URxvt*color15: #ffffff

URxvt*transparent: true
URxvt*shading: 25

! Colored man pages?  
URxvt.colorIT: #87af5f
URxvt.colorBD: #d7d7d7
URxvt.colorUL: #87afd7

! Disable ctrl-shift hostile takeover
URxvt.iso14755: false
URxvt.iso14755_52: false

! nice things
URxvt*urgentOnBell: true
URxvt*visualBell: true
URxvt*scrollWithBuffer: false
URxvt*scrollTtyKeypress: false
